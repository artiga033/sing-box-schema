{
    "allOf": [
        {
            "properties": {
                "type": {
                    "const": "shadowsocks"
                },
                "tag": {
                    "type": "string",
                    "description": "The tag of the outbound."
                },
                "server": {
                    "type": "string",
                    "description": "The server address."
                },
                "server_port": {
                    "type": "integer",
                    "description": "The server port."
                },
                "method": {
                    "type": "string",
                    "enum": [
                        "2022-blake3-aes-128-gcm",
                        "2022-blake3-aes-256-gcm",
                        "2022-blake3-chacha20-poly1305",
                        "none",
                        "aes-128-gcm",
                        "aes-192-gcm",
                        "aes-256-gcm",
                        "chacha20-ietf-poly1305",
                        "xchacha20-ietf-poly1305",
                        "aes-128-ctr",
                        "aes-192-ctr",
                        "aes-256-ctr",
                        "aes-128-cfb",
                        "aes-192-cfb",
                        "aes-256-cfb",
                        "rc4-md5",
                        "chacha20-ietf",
                        "xchacha20"
                    ],
                    "description": "Encryption method."
                },
                "password": {
                    "type": "string",
                    "description": "The shadowsocks password."
                },
                "plugin": {
                    "type": "string",
                    "description": "Shadowsocks SIP003 plugin.\n\nOnly `obfs-local` and `v2ray-plugin` are supported."
                },
                "plugin_opts": {
                    "type": "string",
                    "description": "Shadowsocks SIP003 plugin options."
                },
                "network": {
                    "type": "string",
                    "enum": ["tcp", "udp"],
                    "description": "Enabled network.\n\nOne of `tcp` `udp`.\n\nBoth is enabled by default."
                },
                "udp_over_tcp": {
                    "$ref": "../shared/udp-over-tcp.schema.json"
                },
                "multiplex": {
                    "$ref": "../shared/multiplex-outbound.schema.json"
                }
            },
            "required": ["type", "server", "server_port", "method", "password"]
        },
        {
            "$ref": "../shared/dial.schema.json"
        }
    ]
}
