{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "https://artiga033.github.io/sing-box-schema/outbound/hysteria2.schema.json",
	"allOf": [
		{
			"properties": {
				"type": {
					"const": "hysteria2"
				},
				"tag": {
					"type": "string",
					"description": "The tag of the outbound."
				},
				"server": {
					"type": "string",
					"description": "The server address."
				},
				"server_port": {
					"type": "integer",
					"description": "The server port."
				},
				"server_ports": {
					"$ref": "../types/string-or-array.schema.json",
					"description": "ðŸ†• Since sing-box 1.11.0\n\nServer port range list."
				},
				"hop_interval": {
					"type": "string",
					"description": "ðŸ†• Since sing-box 1.11.0\n\nPort hopping interval.\n\n`30s` is used by default."
				},
				"up_mbps": {
					"type": "integer",
					"description": "Max upload bandwidth in Mbps."
				},
				"down_mbps": {
					"type": "integer",
					"description": "Max download bandwidth in Mbps."
				},
				"obfs": {
					"type": "object",
					"properties": {
						"type": {
							"type": "string",
							"enum": ["salamander"],
							"description": "QUIC traffic obfuscator type."
						},
						"password": {
							"type": "string",
							"description": "QUIC traffic obfuscator password."
						}
					}
				},
				"password": {
					"type": "string",
					"description": "Authentication password."
				},
				"network": {
					"type": "string",
					"enum": ["tcp", "udp"],
					"description": "Enabled network.\n\nOne of `tcp` `udp`.\n\nBoth is enabled by default."
				},
				"tls": {
					"$ref": "../shared/tls-outbound.schema.json"
				},
				"brutal_debug": {
					"type": "boolean",
					"description": "Enable debug information logging for Hysteria Brutal CC."
				}
			},
			"required": ["type", "server", "tls"]
		},
		{
			"$ref": "../shared/dial.schema.json"
		}
	]
}
