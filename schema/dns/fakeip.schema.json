{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "https://artiga033.github.io/sing-box-schema/dns/fakeip.schema.json",
	"type": "object",
	"properties": {
		"enabled": {
			"type": "boolean",
			"description": "Enable FakeIP service."
		},
		"inet4_range": {
			"type": "string",
			"description": "IPv4 address range for FakeIP.",
			"pattern": "(\\d{1,3}(\\.\\d{1,3}){3})(\\/\\d{1,2})?"
		},
		"inet6_range": {
			"type": "string",
			"description": "IPv6 address range for FakeIP.",
			"pattern": "([a-fA-F0-9:]+)(\\/\\d{1,3})?"
		}
	}
}
